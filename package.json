{
  "name": "easycollapse",
	"displayName": "easyCollapse",
	"description": "Collapse code automatically when VSCode starts to help organise your workspace",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"onCommand:easycollapse.easycollapse"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "easycollapse.easycollapse",
            "title": "Collapse Functions"

		}],
		"keybindings": [{
			"command": "easycollapse.easycollapse",
			"key": "f12",
			"mac": "f12",
			"when": "editorTextFocus"
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"icon": "icon.png",
	"homepage": "https://github.com/FAXES/easycollapse",
    "repository": {
        "type": "git",
        "url": "https://github.com/FAXES/easycollapse"
    },
    "bugs": {
        "url": "https://github.com/FAXES/easycollapse/issues"
    },
	"publisher": "faxes",
	"devDependencies": {
        "@types/vscode": "^1.64.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "eslint": "^8.6.0",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "typescript": "^4.5.4",
        "@vscode/test-electron": "^2.0.3"
    }
}
